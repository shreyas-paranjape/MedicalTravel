<div class="row procedure">
	<div class="col-lg-8 border no-padding">

		<div class="row doc" >
			<div class="col-lg-1">
			</div>
			<div class="col-lg-3">
				<img src="images/logo.png" >
			</div>
			<div class="col-lg-4">
				{{#each doctor}}
					<h3>{{name}}</h3>
					<p>{{{summary}}}</p>
				{{/each}}
			</div>
			<div class="col-lg-4">
				<!-- <input type="text" class="form-control" id="text" value="Verified"> -->
				<a href="" id="link">Verified</a>
			</div>
		</div>
		<div class="scrolls" data-spy="scroll" data-target="#navbar-example">
				<nav class="navbar navbar-default" >
				<ul class="nav navbar-nav row" style="padding-top: 8px;">
					<li class="col-lg-1">&nbsp;</li>
					<li class="col-lg-2"><a href="#bio">Bio</a></li>
					<li class="col-lg-3" ><a href="#procedures">Procedure</a></li>
					<li class="col-lg-3"><a href="#providers" style="margin-left:7px;" >Providers</a></li>
					<li class="col-lg-2"><a href="#reviews" >Reviews</a></li>
				</ul>
			</nav>

			<h4 id="bio"></h4>
			<!-- <div class="row">
				<div class="col-lg-1">
				</div>
				<div class="col-lg-10">

					{{#each provider}}
						<p>
							{{{description}}}
						</p>
					{{/each}}
				</div>
