								<div class="collapse navbar-collapse" style="padding-right: 15px;">
									<ul class="nav navbar-nav navbar-right">
										{{#each navLinks}}
										<li {{#ifeq ../section key}}class="active" {{else}}{{/ifeq}} style="font-family: SourceSansPro-Regular;font-size:16.5px;padding-left:20px;">
											<a href="{{ href }}" style="outline:none;color:rgb(235,235,235);background-color:rgba(0,0,0,0);">{{ label }}</a>
