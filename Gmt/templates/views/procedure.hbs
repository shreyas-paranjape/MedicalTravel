<!-- <div class="container"> -->
<div class="row procedure"> <!-- row starts-->
	<div class="col-lg-8 border no-padding">
		<img src="../images/cancer_Treatments(1).jpg" >
		<div class="scrolls" data-spy="scroll" data-target="#navbar-example">
			<nav class="navbar navbar-default">
				<ul class="nav navbar-nav row" style="padding-top: 8px;">
					<li class="col-lg-1">&nbsp;</li>
					<li class="col-lg-2" ><a href="#overview">Overview</a></li>
					<li class="col-lg-4" style="margin-left: 15px;"><a href="#providers">Featured Providers</a></li>
					<li class="col-lg-2"><a href="#doctors" style="margin-left:-28px;">Specialists</a></li>
					<li class="col-lg-2"><a href="#procedures" style="margin-left: -11px;">Procedures</a></li>
				</ul>
			</nav>
			<h4 id="overview"></h4>
			<div class="row">
				<div class="col-lg-1">
				</div>
				<div class="col-lg-10">
					<p> {{{speciality}}}</p>
				</div>
				<div class="col-lg-1">
				</div>
			</div>
			<h4 id="providers">
				<div class="row">
					<div class="col-lg-1">
					</div>
		<div class="col-lg-3">
				<span >Featured Providers</span>
			</div>
				<div class="col-lg-8">
				</div>
			</div>
			</h4>
			{{#each providers}}
				<div class="row procedure-box ">
					<div class="col-lg-1">
					</div>
					<div class="col-lg-3">
						<img src="" >
					</div>
					<div class="col-lg-5">
						<h3>{{name}}</h3>
						<button type="button" class="btn btn-primary btn-md raised" style=" ">Know More</button>
						<!-- <a href=""><p>Know More</p></a> -->
						<!-- <button type="button" class="bttn" onclick=location.href="/provider_info/">Know More</button> -->
					</div>
					<div class="col-lg-2">
					</div>
				</div>
			{{/each}}
			<h4 id="doctors">
				<div class="row">
					<div class="col-lg-1">
					</div>
		<div class="col-lg-3">
				<span>Doctors</span>
			</div>
				<div class="col-lg-8">
				</div>
			</div>
			</h4>
			<div class="q">
				<div class="p">
					{{#each doctors}}
						<div class="row proc">
							<div class="col-lg-1">
							</div>
							<div class="col-lg-2">
								<img src="http://placehold.it/104x104">
							</div>
							<div class="col-lg-5 procedure-doc">
								<h4><a href="/doctor/{{key}}">{{name}}</a></h4>
								{{{description}}}
							</div>
							<div class="col-lg-2">
								<!-- <input type="text" class="form-control" id="text" value="View Profile"> -->
								<a href="" id="link" style="">View Profile</a>
							</div>
							<div class="col-lg-1">
							</div>
						</div>
					{{/each}}
				</div>
			</div>
			<h4 id="procedures">
				<div class="row">
					<div class="col-lg-1">
					</div>
		<div class="col-lg-3">
				<span>Procedures</span>
			</div>
				<div class="col-lg-8">
				</div>
			</div>
			</h4>
			<div class="row">
				<div class="col-lg-1">
				</div>
				<div class="col-lg-10" style="border:1px solid #e5e5e5;border-radius:4px;">
				{{#each procedures}}
					<div class="panel-group" id="accordion" role="tablist" aria-multiselectable="true">
						<div class="panel panel-default">
							<div class="panel-heading" role="tab" id="headingOne">
								<h4 class="panel-title">
									<a role="button" data-toggle="collapse" data-parent="#accordion" href="#{{key}}" aria-expanded="false" aria-controls="collapseOne">
										<div class="row accordion" >
											<div class="col-lg-8 title">
												{{name}}
												<!-- <span class="fa fa-plus-square-o"></span> -->
											</div>
											<div class="col-lg-2 price">
												{{price}}
											</div>
											<div class="col-lg-2">
												<input class="btn btn-default" type="submit" value="Enquiry">
											</div>
										</div>
									</a>
								</h4>
								</div>
								<div id="{{key}}" class="panel-collapse collapse " role="tabpanel" aria-labelledby="headingOne">
									<div class="panel-body">
										{{{description}}}
									</div>
								</div>
							</div>
						</div>
				{{/each}}
			</div>
				<div class="col-lg-1">
				</div>
				</div>
			</div>
		</div>

	<div class="col-lg-4 no-padding"> <!--col 4 starts-->
		<ul class="nav nav-stacked tabs">
			{{#each specialities}}
				{{#if active}}
					<li role="presentation" class="active">
						<a href="/procedure/{{key}}" >
							<img src="/images/{{key}}.png" style="width:27px;height:28.1px;margin-left:-60px;margin-top:0px;margin-right:20px;position:relative;top:8px">
							<span>{{name}}</span>
						</a>
					</li>
				{{else}}
					<li role="presentation">
						<a href="/procedure/{{key}}" style="">
							<img src="/images/{{key}}.png" style="width:27px;height:28.1px;margin-left:-60px;margin-top:0px;margin-right:20px;position:relative;top:8px">
							<span>{{name}}</span>
						</a>
					</li>
				{{/if}}
			{{/each}}
		</ul>
	</div><!--col 4 ends-->
</div> <!--row ends-->
