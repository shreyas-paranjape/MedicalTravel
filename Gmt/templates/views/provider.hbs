<!-- <div class="container"> -->
{{>header}}
<div class="row marginAlign">
	<div class="col-lg-8 col-sm-8 col-xs-8 col-md-8 border no-padding">

		<div id="carousel-example-generic" class="carousel slide" data-ride="carousel">
			<!-- Indicators -->
			<ol class="carousel-indicators">
				<li data-target="#carousel-example-generic" data-slide-to="0" class="active"></li>
				<li data-target="#carousel-example-generic" data-slide-to="1"></li>
			</ol>

			<!-- Wrapper for slides -->
			<div class="carousel-inner" role="listbox">
				<div class="item active">
					<img src="../images/cancer_Treatments(1).jpg" style="width:100%;height:auto">
					<div class="carousel-caption">
						1
					</div>
				</div>
				<div class="item">
					<img src="../images/cancer_Treatments(1).jpg" style="width:100%;height:auto">
					<div class="carousel-caption">
						2
					</div>
				</div>
			</div>

			<!-- Controls -->
			<a class="left carousel-control" href="#carousel-example-generic" role="button" data-slide="prev">
				<span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
				<span class="sr-only">Previous</span>
			</a>
			<a class="right carousel-control" href="#carousel-example-generic" role="button" data-slide="next">
				<span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
				<span class="sr-only">Next</span>
			</a>
		</div>

		<!-- <div class="scrolls" data-spy="scroll" data-target="#navbar-example"> -->
		<nav class="navbar navbar-default" id="navprovider">
			<ul id="myNavbar" class="nav navbar-nav row" style="padding-top: 8px;">
				<li class="col-lg-1 col-sm-1 col-xs-1 col-md-1">&nbsp;</li>
				<li data-menuanchor="about" class="col-lg-2 col-sm-2 col-xs-2 col-md-2 ">
					<a class="demo" href="#section1">About</a>
				</li>
				<li data-menuanchor="doctors" class="col-lg-2 col-sm-2 col-xs-2 col-md-2">
					<a class="demo" href="#section2">Doctors</a>
				</li>
				<li data-menuanchor="speciality" class="col-lg-3 col-sm-3 col-xs-3 col-md-3">
					<a class="demo" href="#section3" style="margin-left:7px;">Speciality</a>
				</li>
				<li data-menuanchor="reviews" class="col-lg-2 col-sm-2 col-xs-2 col-md-2">
					<a class="demo" href="#section4">Reviews</a>
				</li>
			</ul>
		</nav>

		<div id="navstick">
			<section id="section1">
				<h4 id=""></h4>
				<div class="row ">
					<div class="col-lg-1 col-sm-1 col-xs-1 col-md-1">
					</div>
					<div class="col-lg-10 col-sm-10 col-xs-10 col-md-10 para">
						{{{provider.description}}}
					</div>
					<div class="col-lg-1 col-sm-1 col-xs-1 col-md-1">
					</div>
				</div>
			</section>

			<section id="section2">
				<h4 id="over">
					<div class="row">
						<div class="col-lg-1 col-sm-1 col-xs-1 col-md-1">
						</div>
						<div class="col-lg-3 col-sm-3 col-xs-3 col-md-3">
							<span>Doctors</span>
						</div>
						<div class="col-lg-8 col-sm-8 col-xs-8 col-md-8">
						</div>
					</div>
				</h4>
				<div class="q">
					<div class="p">
						{{#each provider.doctors}}
							<div class="row proc">
								<div class="col-lg-1 col-sm-1 col-xs-5 col-md-1">
								</div>
								<div class="col-lg-2 col-sm-12 col-xs-2 col-md-2">
									<img src="" >
								</div>
								<div class="col-lg-6 col-sm-12 col-xs-6 col-md-6 procedure-doc">
									<h4><a href="/doctor/{{key}}">{{name}}</a></h4>
									{{{summary}}}
								</div>
								<div class="col-lg-2 col-sm-12 col-xs-2 col-md-2">
									<span>
										<input class="btn btn-primary btn-md rai" type="submit" value="View Profile" style="outline:none;" onclick="location.href='/doctor/{{key}}'">
									</span>
								</div>
								<div class="col-lg-1 col-sm-1 col-xs-1 col-md-1">
								</div>
							</div>
						{{/each}}
					</div>
				</div>
			</section>

			<section id="section3">
				<h4 id="over">
					<div class="row">
						<div class="col-lg-1 col-sm-1 col-xs-1 col-md-1">
						</div>
						<div class="col-lg-3 col-sm-3 col-xs-3 col-md-3">
							<span>Speciality</span>
						</div>
						<div class="col-lg-8 col-sm-8 col-xs-8 col-md-8">
						</div>
					</div>
				</h4>

				<div class="row doc_proc">
					<div class="col-lg-1 col-sm-1 col-xs-1 col-md-1">
					</div>
					<div class="col-lg-10 col-sm-10 col-xs-10 col-md-10 doc_border">
						<div class="panel-group" id="accordion" role="tablist" aria-multiselectable="true">
							{{#each specialities}}

								<div class="panel panel-default">
									<div class="panel-heading" role="tab" id="headingOne">
										<h4 class="panel-title">
											<a role="button" data-toggle="collapse" data-parent="#accordion" href="#{{key}}" aria-expanded="false" aria-controls="collapseOne">
												<div class="row accordion">
													<div class="col-lg-11 col-sm-11 col-xs-11 col-md-11 title">
														{{name}}
													</div>
													<div class="col-lg-1 col-sm-1 col-xs-1 col-md-1 icon" style="color: #d6d2d2;">
														<span class="glyphicon glyphicon-chevron-down"></span>
													</div>
												</div>
											</a>
										</h4>
									</div>
									<div id="{{key}}" class="panel-collapse collapse " role="tabpanel" aria-labelledby="headingOne">
										<div class="panel-body">
											{{#each procedures}}
												<div class="row provider_procedures">
													<div class="col-lg-8 col-sm-8 col-xs-8 col-md-8 title">
														<span>{{name}}</span>
													</div>
													<div class="col-lg-2 col-sm-2 col-xs-2 col-md-2 price">
														<span>â‚¹ {{price}}</span>
													</div>
													<div class="col-lg-2 col-sm-2 col-xs-2 col-md-2">
														<input class="btn btn-primary btn-md procedure_btn" type="submit" value="Enquiry" onclick="location.href='/data/{{key}}'">
													</div>
												</div>
											{{/each}}
										</div>
									</div>
								</div>
							{{/each}}
						</div>
					</div>
					<div class="col-lg-1 col-sm-1 col-xs-1 col-md-1">
					</div>
				</div>
			</section>

			<section id="section4">
				<h4 id="over">
					<div class="row">
						<div class="col-lg-1 col-sm-1 col-xs-1 col-md-1">
						</div>
						<div class="col-lg-8 col-sm-8 col-xs-8 col-md-8">
							<span>Reviews</span>
						</div>
						<div class="col-lg-3 col-sm-3 col-xs-3 col-md-3">
						</div>
					</div>
				</h4>
				<!-- <span class="glyphicon glyphicon-star" style="margin-left:30px"></span>
			<span class="glyphicon glyphicon-star"></span>
			<span class="glyphicon glyphicon-star"></span> -->
				{{#each provider.reviews}}

					<div> {{{comments}}} --> {{hits}}
					</div>
				{{/each}}
			</section>
		</div>
		<!-- </div> -->
	</div>

	<div class="col-lg-4 col-sm-4 col-xs-4 col-md-4 no-padding name">
		<h3>{{provider.name}}</h3>


		<div class="form frm_doctor" style="margin-top:-10px;background-color: #f6f6f6;margin-left:-1px;">
			<h5>Request a Free Quote</h5>
			<div class="row no-margin">
				<div class="col-lg-1 col-md-1 col-sm-1 col-xs-1">
				</div>
				<div class="col-lg-10 col-md-10 col-sm-10 col-xs-10">
					<form method="post" enctype="multipart/form-data">
						<input type="hidden" name="action" value="second">
						<textarea class="form-control" rows="5" placeholder="Case info" name="caseinfo" style="margin-top: 15px;border-radius:8px;"></textarea>
						<div class="panel-group doc_acc" id="accordion1" class="reports" role="tablist" aria-multiselectable="true">
							<div class="panel panel-default" style="">
								<div class="panel-heading" role="tab" id="headingOne" style="">
									<a role="button" data-toggle="collapse" data-parent="#accordion1" href="#collapseOne" aria-expanded="true" aria-controls="collapseOne" style="outline:none;">
										<h4 class="panel-title" style="color:black">
											<div class="row form_accordion">
												<div class="col-lg-10 col-md-10 col-sm-10 col-xs-10" style="padding-top:3px">
													<span class="medical" style="">Medical Reports</span>
												</div>
												<div class="col-lg-2 col-md-2 col-sm-2 col-xs-2 icon">
													<span style="" class="glyphicon glyphicon-chevron-down"></span>
												</div>
											</div>
										</h4>
									</a>
								</div>
								<div id="collapseOne" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingOne">
									<div class="panel-body" style="background-color:rgb(237,237,237);border-top-width: 0px; ">
										<input type="file" name="fileupload" size="80" value="Attach" multiple>
									</div>
								</div>
							</div>

							<div class="panel panel-default" style="border: none;box-shadow: none;">
								<div class="panel-heading" role="tab" id="headingTwo" style="margin-top: 5px;padding: 10px 15px;">
									<a class="collapsed" role="button" data-toggle="collapse" data-parent="#accordion1" href="#collapseTwo" aria-expanded="false" aria-controls="collapseTwo" style="outline:none;">
										<h4 class="panel-title" style="color:black">
											<div class="row form_accordion">
												<div class="col-lg-10 col-md-10 col-sm-10 col-xs-10" style="padding-top:3px">
													<span class="medical" style=""> Allergies</span>
												</div>
												<div class="col-lg-2 col-md-2 col-sm-2 col-xs-2 icon">
													<span style="" class="glyphicon glyphicon-chevron-down"></span>
												</div>
											</div>
										</h4>
									</a>
								</div>
								<div id="collapseTwo" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingTwo">
									<div class="panel-body" style="background-color:rgb(237,237,237);border-top-width: 0px;">
										<input type="text" class="form-control" id="exampleInputName1" placeholder="Allergies">
									</div>
								</div>
							</div>

							<div class="panel panel-default" style="border: none;box-shadow: none;">
								<div class="panel-heading" role="tab" id="headingThree" style="margin-top: 5px;padding: 10px 15px;">
									<a class="collapsed" role="button" data-toggle="collapse" data-parent="#accordion1" href="#collapseThree" aria-expanded="false" aria-controls="collapseThree" style="outline:none;">
										<h4 class="panel-title" style="color:black">
											<div class="row form_accordion">
												<div class="col-lg-10 col-md-10 col-sm-10 col-xs-10 title" style="padding-top:3px">
													<span class="medical" style="">Symptoms</span>
												</div>
												<div class="col-lg-2 col-md-2 col-sm-2 col-xs-2 icon">
													<span style="" class="glyphicon glyphicon-chevron-down"></span>
												</div>
											</div>
										</h4>
									</a>
								</div>
								<div id="collapseThree" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingThree">
									<div class="panel-body" style="background-color:rgb(237,237,237);border-top-width: 0px;">
										<input type="text" class="form-control" id="exampleInputName2" placeholder="Symptoms">
									</div>
								</div>
							</div>
						</div>

						<div class="form-group">
							<input type="text" name="name.full" class="form-control" id="name" placeholder="Name:" style="margin-top: -10px;">
						</div>

						<div class="form-group row form_fields">
							<div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
								<input type="text" class="form-control" id="age" placeholder="Age" name="age">
							</div>
							<div class="col-lg-2 col-md-0 col-sm-0 col-xs-2">
							</div>
							<div class="col-lg-7 col-md-8 col-sm-8 col-xs-6" style="padding-top: 10px;">
								<label for="gender" class="gender">Sex:</label>
								<label class="radio-inline">
									<input type="radio" name="gender" class="value" value="Male">M</label>
								<label class="radio-inline">
									<input type="radio" name="gender" class="value" value="Female" >F</label>
								<label class="radio-inline">
									<input type="radio" name="gender" class="value" value="Other" >O</label>
								<!-- <input type="text" class="form-control" id="gender" placeholder="Gender" name="gender" style="padding-top: 10px; padding-bottom: 10px; margin-top: 0px;"> -->
							</div>
						</div>

						<div class="form-group">
							<input type="email" name="email" class="form-control" id="email" placeholder="Email:">
						</div>

						<div class="form-group">
							<input type="phone" name="phone" class="form-control" id="phone" placeholder="Phone(Optional)">
						</div>

						</br>
						<div class="row">
							<div class="col-lg-3 col-md-3 col-sm-3 col-xs-3"></div>
							<div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
								<button type="submit" class="btn btn-default doctor_btn" style="margin-left: 20px; padding: 3px 15px;" >Submit</button>
							</div>
							<div class="col-lg-3 col-md-3 col-sm-3 col-xs-3"></div>
						</div>
					</form>

				</div>
				<div class="col-lg-1 col-md-1 col-sm-1 col-xs-1">
				</div>
			</div>

		</div>




	</div>

</div>
<!-- </div> -->
{{>footer}}
<script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
<script src="/js/smooth-scroller.js"></script>
<script src="/js/mainScript.js"></script>
<script src="/js/pro.js"></script>
<script src="/js/navbar.js"></script>
