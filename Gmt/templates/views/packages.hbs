{{>header}}
<div class="packagePic marginAlign ">
	<div class="row" style="padding-top:205px;">
		<div class="col-md-4 col-lg-4 col-sm-4 col-xs-4">
			<div class="row">
				<div class="col-md-2 col-lg-2 col-sm-2 col-xs-2">
				</div>
				<div class="col-md-10 col-lg-10 col-sm-10 col-xs-12" style="padding-left:15px;">
					<span style="font-size:30px;font-family:SourceSansPro-Semibold;color:white">{{{packageCate}}}</span>
				</div>
			</div>
		</div>
	</div>
</div>

<div class="row procedure">
	<div class="col-lg-8 border ">
		<div class="row">
			<!-- <div class="col-lg-1 col-sm-1 col-xs-1 col-md-1 "></div> -->
			<!-- <div class="col-lg-10 col-sm-10 col-xs-10 col-md-10 "> -->
				<!-- <div class="row package"> -->
					{{#each packages}}
						<!-- <div class="col-lg-1 col-sm-1 col-xs-1 col-md-1 "></div> -->
						<div class="col-lg-4 col-sm-4 col-xs-4 col-md-4" style="padding-right:20px;padding-left:20px;">
							<div class="thumbnail" style="margin-top:30px;margin-bottom:0px;">
								<div class="image" style="position:relative; width:100%">
									<img src="{{cloudinaryUrl imageOuter}}" style="display:block;margin-left:auto;margin-right:auto;width:25%;padding-top: 80px;" align="middle">
										<h2 class=" package-type" style="">{{{type}}}</h2>
								</div>
								<div class="caption package-caption">
									<h3 style="padding-left:8px;font-family:SourceSansPro-Semibold;color:rgb(149,149,149);margin-top:0px;">{{name}}</h3>
									{{{description}}}
									<p><a class="btn btn-default" role="button" style="box-shadow: 0 3px 0 0 rgb(204,86,88);background-color: #ff6c6f;border-color: rgb(255,108,111);color:white;font-family:SourceSansPro-Regular;margin-top:10px;" onclick="location.href='/package/{{key}}'">Know more</a></p>
								</div>
							</div>
						</div>
						<!-- <div class="col-lg-1 col-sm-1 col-xs-1 col-md-1 "></div> -->
					{{/each}}
				<!-- </div> -->
			<!-- </div> -->
			<!-- <div class="col-lg-1 col-sm-1 col-xs-1 col-md-1 "></div> -->
		</div>
	</div>

	<div class="col-lg-4 no-padding">
		<h4 class="specialities" style="background-color:#C6C6C6;color:white;margin-bottom:0px;">Packages</h4>
		<ul class="nav nav-stacked tabs">
			{{#each packageCategory1}}
				{{#if active}}
					<li role="presentation" class="active" style="margin-top: 0px;">
						<a href="/packages/{{key}}">
							<div class="row">
								<div class="col-lg-1 col-sm-1 col-xs-1 col-md-1">
								</div>
								<div class="col-lg-2 col-sm-2 col-xs-2 col-md-2">
									<img src="/images/{{key}}.png">
									<!-- style="width:27px;height:28.1px;position:relative;top:8px" -->
								</div>
								<div class="col-lg-9">
									<span>{{name}}</span>
								</div>

							</div>
						</a>
					</li>
				{{else}}
					<li role="presentation" style="margin-top: 0px;">
						<a href="/packages/{{key}}">
							<div class="row">
								<div class="col-lg-1 col-sm-1 col-xs-1 col-md-1">
								</div>
								<div class="col-lg-2 col-sm-2 col-xs-2 col-md-2">
									<img src="/images/{{key}}.png">
								</div>
								<div class="col-lg-9">
									<span>{{name}}</span>
								</div>
							</div>
						</a>
					</li>
				{{/if}}
			{{/each}}

			<h4 class="specialities" style="background-color:#C6C6C6;color:white;margin-bottom:0px;">Checkups</h4>
			{{#each packageCategory2}}
				{{#if active}}
					<li role="presentation" class="active" style="margin-top: 0px;">
						<a href="/packages/{{key}}">
							<div class="row">
								<div class="col-lg-1 col-sm-1 col-xs-1 col-md-1">
								</div>
								<div class="col-lg-2 col-sm-2 col-xs-2 col-md-2">
									<img src="/images/{{key}}.png">
								</div>
								<div class="col-lg-9">
									<span>{{name}}</span>
								</div>

							</div>
						</a>
					</li>
				{{else}}
					<li role="presentation" style="margin-top: 0px;">
						<a href="/packages/{{key}}">
							<div class="row">
								<div class="col-lg-1 col-sm-1 col-xs-1 col-md-1">
								</div>
								<div class="col-lg-2 col-sm-2 col-xs-2 col-md-2">
									<img src="/images/{{key}}.png">
								</div>
								<div class="col-lg-9">
									<span>{{name}}</span>
								</div>
							</div>
						</a>
					</li>
				{{/if}}
			{{/each}}
		</ul>
	</div>
</div>
{{>footer}}
